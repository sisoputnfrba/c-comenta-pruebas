SET EAX 16
SET EBX 20
RESIZE 128
IO_STDIN_READ TECLADO EAX EBX
SET AX 87
SET BX 65
SET CX 82
SET DX 44
SET ECX 11
SET EDX 1
MOV_OUT ECX AX
SUM ECX EDX
MOV_OUT ECX BX
SUM ECX EDX
MOV_OUT ECX CX
SUM ECX EDX
MOV_OUT ECX DX
SUM ECX EDX
SET AX 32
IO_GEN_SLEEP GENERICA 2
MOV_OUT ECX AX
SET EBX 25
SET EAX 11
IO_STDOUT_WRITE MONITOR EAX EBX
EXIT